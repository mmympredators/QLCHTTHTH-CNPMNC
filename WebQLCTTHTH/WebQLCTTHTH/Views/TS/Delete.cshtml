@model WebQLCTTHTH.Models.ThiSinh

@{
    ViewBag.Title = "Xóa Thí Sinh";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Xóa Thí Sinh</h2>

<h3>Bạn có muốn xóa thí sinh này?</h3>
<div>
    <h4>Thí Sinh</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Họ Tên Thí Sinh
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HoTenTS)
        </dd>

        <dt>
            Năm Sinh
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NamSinhTS)
        </dd>

        <dt>
            Giới Tính
        </dt>

        <dd>
            @Html.DisplayFor(model => model.GT)
        </dd>

        <dt>
            Địa Chỉ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DiaChiTS)
        </dd>

        <dt>
            Quê quán
        </dt>

        <dd>
            @Html.DisplayFor(model => model.QueQuanTS)
        </dd>

        <dt>
            Hình Thí Sinh
        </dt>

        <dd>
            @Html.DisplayFor(model => model.HinhTS)
        </dd>

        <dt>
            Nộp Phí
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NopPhi)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Trở về", "TS")
        </div>
    }
</div>
